# EmailJS Configuration
REACT_APP_EMAILJS_SERVICE_ID=your_service_id_here
REACT_APP_EMAILJS_TEMPLATE_ID_PROD=your_prod_template_id_here
REACT_APP_EMAILJS_TEMPLATE_ID_DEV=your_dev_template_id_here
REACT_APP_EMAILJS_USER_ID=your_user_id_here
REACT_APP_PROVIDER_EMAIL=your_email@example.com

# Firebase Configuration
# IMPORTANT: Generate new keys from Firebase Console > Project Settings > General > Web apps
# Never commit real API keys to version control
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_app_id_here
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Security Notes:
# 1. All environment variables are REQUIRED - no fallbacks provided
# 2. Never commit .env file to version control
# 3. Rotate API keys regularly
# 4. Use different keys for development/staging/production
# 5. Monitor API key usage for unauthorized access

# Admin Configuration
REACT_APP_ADMIN_PASSWORD=your_secure_admin_password_here
